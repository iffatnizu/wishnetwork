}
tr.wish-list{
    background: #fff;
    border-bottom: 1px solid #ccc;
    height: 50px;
}
tr.wish-list td{
    padding: 4px;
}
.item.list-group-item
{
    float: left;
    width: 97.5%;
    background-color:#f5f5f5;
    margin-bottom: 5px;
    border-bottom: 2px solid #A00083;
    text-align: left;
}
.item.list-group-item:before,
.item.list-group-item:after {
    display: table;
    content: " ";
}

.item.list-group-item img{float:left;}
.item.list-group-item:after
{
    clear:both;
}
img.list-group-image{
    width: 200px;
    height: 200px;
    margin-right: 10px;
}
.comment img{
