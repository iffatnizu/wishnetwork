.wishcard {
    background:#FBFBFB;
    background-image: -moz-linear-gradient(center top , #FBFBFB 0%, #FFFFFF 60%) repeat scroll 0 0 rgba(0, 0, 0, 0);
    background-image: -webkit-gradient(linear, left top, left bottom, from(#FBFBFB), to(#FFFFFF));
    background-image: -webkit-linear-gradient(top, #FBFBFB, #FFFFFF);
    background-image:    -moz-linear-gradient(top, #FBFBFB, #FFFFFF);
    background-image:      -o-linear-gradient(top, #FBFBFB, #FFFFFF);
    background-image:         linear-gradient(to bottom, #FBFBFB, #FFFFFF);	
    border: 1px solid #A4A4A4;
    border-radius: 3px;
    box-shadow: 3px 3px 4px #CCCCCC;
    margin: 15px 0px 0 0;
    min-height: 100px;
    position: relative;

}
.wishcard .title p {
    font-size: 16px;
    font-weight: bold;
    max-width: 290px;
    overflow: hidden;
    padding: 5px;
    position: relative;
    text-align: left;
}
.wishcard a .title, .wishcard a:hover .title {
    color: #000000;
    text-decoration: none;
}
.wishcard .wish-pic .verified-wish-pic {
    border: medium none;
    left: 235px;
    margin: 0;
    padding: 0;
    position: absolute;
    top: 5px;
    width: 60px;
}
.wishcard .wish-pic {
    display: block;
    overflow: hidden;
    position: relative;
}
.wishcard .wish-pic img {
    border-bottom: 1px solid #A4A4A4;
    border-top: 1px solid #A4A4A4;

}
.wishcard .wish-cat-loc {
    color: #666666;
    display: inline-block;
    font-size: 12px;
    padding: 8px 10px;
    width: 96%;
}
.wishcard .wish-cat-loc .wish-cat {
    display: inline-block;
    float: left;
    font-weight: bold;
    overflow: hidden;
    text-align: left;
    text-transform: uppercase;
    vertical-align: top;
}
.wishcard .wish-cat-loc .wish-loc {
    display: inline-block;
    float: right;
    max-height: 28px;
    overflow: hidden;
    text-align: right;
    text-transform: capitalize;
    vertical-align: top;
}
.wishcard .wish-desc {
    font-size: 13px;
    line-height: 18px;
    padding: 0 10px;
    text-align: left;
}
.carddaysleft {
    background: #99007E;
    color: #FFF;
    display: inline-block;
    font-family: Arimo,sans-serif;
    font-size: 12px;
    margin: 0;
    padding: 0;
    text-align: center;
    width: 100%;
}
.wishcard .title p:first-line {
    font-size: 20px;
    font-weight: bold;
}.wishcard-progress-bar {
    display: block;
    text-align: left;
}
.wishcard-progress-bar .progress-bar {
    background-color: #EEEEEE;
    padding: 10px 15px 5px;
}

.wishcard .wish-poster {
    color: #999999;
    font-size: 10px;
    padding: 0 5px;
}
.pb-bar.cs_light {
    background: none repeat scroll 0 0 rgba(255, 255, 255, 0.8);
    border: 1px solid #A4A4A4;
    color: #000000;
}
.pb-bar.cs_dark {
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0.8);
    border: 1px solid #363636;
    color: #CCCCCC;
}
.progress-bar {
    display: block;
    position: relative;
    text-align: left;
}
.pb-num {
    font-size: 12px;
    font-weight: bold;
    vertical-align: middle;
}
.pb-desc {
    font-size: 10px;
    text-transform: uppercase;
    vertical-align: middle;
}
.pb-bar {
    border-radius: 5px;
    box-shadow: 3px 3px 4px rgba(0, 0, 0, 0.1) inset;
    height: 15px;
    margin: auto auto 3px;
    position: relative;
}
.pb-fill, .pb-fill-left, .pb-fill-right {
    background-color: #A30086;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#8B0073), to(#A30086));
    background-image: -webkit-linear-gradient(top, #8B0073, #A30086);
    background-image:    -moz-linear-gradient(top, #8B0073, #A30086);
    background-image:      -o-linear-gradient(top, #8B0073, #A30086);
    background-image:         linear-gradient(to bottom, #8B0073, #A30086);
    display: block;
    height: 15px;
    position: absolute;
    z-index: 5;
}
.pb-fill {
    margin-left: 2%;
    margin-right: 5px;
}
.pb-fill-left {
    border-bottom-left-radius: 4px;
    border-top-left-radius: 4px;
    left: 0;
    width: 2%;
}
.pb-fill-right {
    border-bottom-right-radius: 4px;
    border-top-right-radius: 4px;
    right: 0;
    width: 2%;
}
.pb-raised {
    margin: -2px 10px 0;
    position: absolute;
    z-index: 10;
}
.pb-goal {
    float: right;
}
.pb-wrap.shrink {
    padding: 5px 120px 5px 0;
}
.pb-st-right {
    color: #FFFFFF;
    position: absolute;
    right: 0;
    text-align: center;
    top: 0;
    width: 120px;
}
.pb-st-right-inner {
    display: inline-block;
    text-align: right;
}
.pb-st-right .pb-progress {
    display: inline-block;
    font-size: 16px;
    font-weight: bold;
    margin: 3px 5px 0 0;
    padding: 0;
    vertical-align: top;
}
.pb-st-right .pb-goal {
    display: inline-block;
    margin: 0;
    padding: 0;
    text-align: center;
    vertical-align: top;
}
.pb-st-right .pb-goal-ln1 {
    color: #AAAAAA;
    display: block;
    font-size: 10px;
}
.pb-st-right .pb-goal-ln2 {
    color: #CCCCCC;
    display: block;
    font-size: 12px;
}
h3.title{
    color: #421C52;
    float: left;
    font-size: 20px;
    margin: 0;
}
select{
    border-radius: 4px;
    border: 1px solid #ccc;
    padding: 6px;
    width: 70px;
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.15) inset;
}
span.required{
    font-size: 11px;
    color: #99007E;
}
span.error{
    font-size: 11px;
    color: #99007E;
}
.profile-tabs{
    background: #D3C5E7;
    height: 38px;
    border:1px solid #ccc;
}
.profile-tabs ul{
    margin: 0px;
    padding: 0px;
}
.profile-tabs ul li{
    margin: 0px;
    padding: 0px;
    float: left;
